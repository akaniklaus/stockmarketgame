<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_5.xsd"
	version="5">
	<description>Real-time Stock Market Game</description>
	<display-name>STMGame</display-name>

	<!-- Modules -->
	<!-- The default module should be listed first -->
	<module>
		<web>
			<web-uri>default</web-uri>
			<context-root>/default</context-root>
		</web>
	</module>
	<module>
		<web>
			<web-uri>s-updater</web-uri>
			<context-root>/s-updater</context-root>
		</web>
	</module>

</application>
